@using PersonelProje.Data

@model Ulke

<div class="row">
    <div class="col-md-5">
        <h1>Ulke Ekle</h1>
        <hr />
        <form method="post">
            <div>
                <label>Id</label>
                <input class="form-control" type="text" value="@Model.Id" name="Id" />
            </div>
            <div>
                <input class="form-control" type="text" value="@Model.UlkeAd" name="UlkeAd" />
            </div>
            <input class="btn btn-success" type="submit" value="Ekle" />
        </form>
    </div>
</div>

