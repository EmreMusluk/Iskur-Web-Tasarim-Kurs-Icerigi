@model PersonelProje.Data.Sehir

<div class="row">
    <div class="col-md-5">
        <h1>Sehir Sil</h1>
        <hr />
        <form method="post">
            <div>
                <input class="form-control" type="hidden" value="@Model.Id" name="Id" />
            </div>
